{
  "project": {
    "version": "1.0.0-beta.31",
    "name": "PROJECT_NAME"
  },
  "apps": [
    {
      "root": "web",
      "outDir": "public",
      "assets": [
        "media",
        "views"
      ],
      "index": "index.html",
      "main": "app/bootstrap.ts",
      "test": "spec/test.ts",
      "tsconfig": "tsconfig.json",
      "mobile": false,
      "styles": [
          "style/index.less"
      ],
      "scripts": [
      ],
      "environments": {
          "source": "environments/environment.ts",
          "dev": "environments/environment.ts",
          "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [
      "vendor/fonts/*.+(otf|eot|svg|ttf|woff|woff2)"
  ],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "less",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": true
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
